# Streamlit Cloud Secrets Template
# Copy these values to Streamlit Cloud Dashboard > App Settings > Secrets

# Google Earth Engine (REQUIRED)
GEE_PROJECT_ID = "your-gee-project-id"

# Groq AI API (FREE - get key at https://console.groq.com/keys)
GROQ_API_KEY = "your-groq-api-key"

# Optional: NASA Earthdata
EARTHDATA_USERNAME = "your-username"
EARTHDATA_PASSWORD = "your-password"
